{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://www.our-example-data.org/janitor.json",
    "title": "Janitor",
    "description": "A janitor",
    "type": "object",
    "properties": {
        "FirstName": {
            "description": "First name of the janitor",
            "type": "string"
        },
        "MiddleName": {
            "description": "Middle name(s) of the janitor",
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "LastName": {
            "description": "Last name of the janitor",
            "type": "string"
        },
        "id": {
            "description": "Unique identifier of the janitor",
            "type": "integer",
            "exclusiveMinimum": 0
        },
        "OwnsBroom": {
            "description": "Whether or not the janitor owns a broom",
            "type": "boolean"
        }
    },
    "required": ["FirstName", "LastName", "id", "OwnsBroom"]
}