            "@container": "@language"
        },
        "webPage": {
            "@context": {
                "url": "@id"
            },
            "@id": "foaf:homepage"
        },
        "name": {
          	"@id": "foaf:name",
          	"@container": "@language"
        }
    },
    "@id": "uni:Harvard",
    "@type": "ex:University",
    "id": 3,
    "Name": {
        "en": "Harvard"
    },
    "webPage": {
        "url": "https://pll.harvard.edu",
        "@type": "foaf:Document",
        "name": {
            "en": "Harvard"
        }
    }
}